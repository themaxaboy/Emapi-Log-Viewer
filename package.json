{
  "name": "emapi-log-viewer",
  "productName": "Emapi Log Viewer",
  "version": "1.0.0",
  "description": "Emapi Log Viewer",
  "license": "MIT",
  "author": "Max Veerapat Kumchom",
  "main": "main.js",
  "build": {
    "appId": "dst.emapi-log-viewer",
    "directories": {
      "output" : "./release"
    }
  },
  "nodeGypRebuild": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "pack": "electron-builder --win"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-builder": "^19.45.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "style-loader": "^0.19.0",
    "styled-jsx": "^2.2.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "alasql": "^0.4.3",
    "antd": "^2.13.10",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-json-view": "^1.13.3"
  }
}
